  },
  "devDependencies": {
    "@eslint/js": "^9.33.0",
    "@sveltejs/vite-plugin-svelte": "^6.1.2",
    "@tsconfig/svelte": "^5.0.4",
    "@types/node": "^24.3.0",
    "concurrently": "^8.2.2",
    "eslint": "^9.33.0",
    "eslint-plugin-svelte": "^3.11.0",
    "globals": "^16.3.0",
    "monaco-editor": "^0.52.2",
